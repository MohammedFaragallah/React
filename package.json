{
    "name": "follow",
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}",
            "!<rootDir>/node_modules/"
        ],
        "coverageReporters": [
            "json",
            "lcov",
            "text"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ]
    },
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "firebase": "^5.4.2",
        "moment": "^2.22.2",
        "numeral": "^2.0.6",
        "react": "^16.5.0",
        "react-dates": "^18.0.4",
        "react-dom": "^16.5.0",
        "react-modal": "^3.5.1",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.3.1",
        "react-scripts": "1.1.5",
        "react-with-direction": "^1.3.0",
        "redux": "^4.0.0",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "enzyme": "^3.6.0",
        "enzyme-adapter-react-16": "^1.5.0",
        "enzyme-to-json": "^3.3.4",
        "eslint-plugin-prettier": "^2.6.2",
        "npm-run-all": "^4.1.3",
        "prettier": "1.14.2"
    },
    "scripts": {
        "sass:watch": "sass --watch src/App.scss:src/App.css",
        "sass:build": "sass --update src/App.scss:src/App.css",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "frego": "npm-run-all -p sass:watch start"
    }
}
