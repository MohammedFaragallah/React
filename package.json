{
    "name": "follow",
    "homepage": "https://mohammedfaragallah.github.io/React/",
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}",
            "!<rootDir>/node_modules/"
        ],
        "coverageReporters": [
            "json",
            "lcov",
            "text"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ]
    },
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "firebase": "^5.5.1",
        "moment": "^2.22.2",
        "numeral": "^2.0.6",
        "prop-types": "^15.6.2",
        "react": "^16.5.2",
        "react-dates": "^18.1.0",
        "react-dom": "^16.5.2",
        "react-modal": "^3.5.1",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.3.1",
        "react-scripts": "1.1.5",
        "react-scripts-cssmodules": "^1.1.50",
        "react-with-direction": "^1.3.0",
        "redux": "^4.0.0",
        "redux-thunk": "^2.3.0",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "axios": "^0.18.0",
        "coveralls": "^3.0.2",
        "enzyme": "^3.6.0",
        "enzyme-adapter-react-16": "^1.5.0",
        "enzyme-to-json": "^3.3.4",
        "eslint-plugin-prettier": "^2.6.2",
        "gh-pages": "^2.0.0",
        "node-sass-chokidar": "^1.3.3",
        "npm-run-all": "^4.1.3",
        "prettier": "1.14.3"
    },
    "scripts": {
        "build-css": "node-sass-chokidar src/ -o src/",
        "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
        "sass:watch": "sass --watch src/App.scss:src/App.css",
        "sass:build": "sass --update src/App.scss:src/App.css",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "frego": "npm-run-all -p watch-css start"
    }
}